{
  "name": "sinth",
  "version": "1.0.0",
  "description": "Sinth - yt-downloader",
  "main": "src/main.js",
  "author": "Jhonatan Geronimo",
  "scripts": {
    "start": "electron .",
    "build": "electron-builder"
  },
  "build": {
    "appId": "com.sinth.downloader",
    "productName": "Sinth",
    "files": [
      "src/**/*",
      "resources/**/*"
    ],
    "extraResources": [
      {
        "from": "resources/bin/win/",
        "to": "bin/win"
      }
    ],
    "win": {
      "target": "nsis"
    }
  },
  "dependencies": {
    "electron-store": "^8.0.0"
  },
  "devDependencies": {
    "electron": "^31.0.0",
    "electron-builder": "^25.0.0"
  }
}
